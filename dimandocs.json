{
  "directories": [
    {
      "path": "./",
      "name": "Project documents",
      "file_pattern": "\\.md$"
    }
  ],
  "port": "8090",
  "title": "Documentation Browser",
  "ignore_patterns": [
    ".*/\\.git(/.*)?$",
    ".*/\\.svn(/.*)?$",
    ".*/\\.hg(/.*)?$",
    ".*/node_modules(/.*)?$",
    ".*/vendor(/.*)?$",
    ".*/bower_components(/.*)?$",
    ".*/build(/.*)?$",
    ".*/dist(/.*)?$",
    ".*/out(/.*)?$",
    ".*/target(/.*)?$",
    ".*/\\.next(/.*)?$",
    ".*/\\.nuxt(/.*)?$",
    ".*/\\.vuepress(/.*)?$",
    ".*/\\.cache(/.*)?$",
    ".*/__pycache__(/.*)?$",
    ".*/\\.pytest_cache(/.*)?$",
    ".*/\\.nyc_output(/.*)?$",
    ".*/\\.vscode(/.*)?$",
    ".*/\\.idea(/.*)?$",
    ".*/\\.eclipse(/.*)?$",
    ".*/venv(/.*)?$",
    ".*/env(/.*)?$",
    ".*/.venv(/.*)?$",
    ".*/\\.virtualenv(/.*)?$",
    ".*/coverage(/.*)?$",
    ".*/htmlcov(/.*)?$",
    ".*/tmp(/.*)?$",
    ".*/temp(/.*)?$",
    ".*/.tmp(/.*)?$"
  ]
}
